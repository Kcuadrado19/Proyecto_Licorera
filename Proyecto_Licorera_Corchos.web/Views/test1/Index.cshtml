@model List<test1>
@{
    ViewData["Title"] = "test1";
}

<h1>test1</h1>

<a class="btn btn-primary" asp-action="Create">+ Nuevo Autor</a>

<table class="table table-bordered mt-3">

    <thead>
        <tr>
            <th>Id</th>
            <th>Nombres</th>
            <th>Apellidos</th>
            <th>Acciones</th>
        </tr>
    </thead>


    <tbody>
        @foreach (test1 test1 in Model)
        {
            <tr>
                <td>@test1.Id</td>
                <td>@test1.FirstName</td>
                <td>@test1.LastName</td>

                <td>
                    <a class="btn btn-warning" asp-action="Edit" asp-route-id="@test1.Id">Editar </a>
                    <a class="btn btn-danger" asp-action="Delete" asp-route-id="@test1.Id">Eliminar </a>
                </td>
            </tr>

        }

    </tbody>


</table>