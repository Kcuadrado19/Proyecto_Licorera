@model List<string>

@{
    ViewData["Title"] = "Permisos - Licorería";
}

<h2>Administrar Permisos</h2>

<p>Aquí puedes gestionar los permisos asignados a los roles.</p>

@foreach (var permission in Model)
{
    <div class="form-check">
        <input class="form-check-input" type="checkbox" id="@permission" />
        <label class="form-check-label" for="@permission">@permission</label>
    </div>
}

<a asp-action="Index" class="btn btn-secondary mt-3">Volver a Roles</a>

